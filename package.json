{
  "name": "powerball-pro",
  "version": "1.0.0",
  "description": "powerball app",
  "main": "server.js",
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^1.18.7"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JuanCodeAtATime/powerball-pro.git"
  },
  "author": "Juan Rivera",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JuanCodeAtATime/powerball-pro/issues"
  },
  "homepage": "https://github.com/JuanCodeAtATime/powerball-pro#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-passport-logout": "^0.1.0",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "mongoose": "^5.9.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local-mongoose": "^6.0.1",
    "passport-validator": "^1.2.3",
    "react-moment": "^0.9.7",
    "react-router-dom": "^5.1.2",
    "validator": "^12.2.0"
  }
}
